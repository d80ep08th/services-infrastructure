{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions" : [
        {
            "name": "telegram-bot",
            "image" : "tattletech/archive-telegram-bot:latest",
            "memory": 256,
            "memoryReservation": 256,
            "portMappings": [
                {
                    "containerPort": 8443, 
                    "hostPort": 3000
                }
            ]
        },
        {
            "name": "factcheck-scraper-api",
            "essential": true, 
            "memory": 256,
            "memoryReservation": 256,
            "image":"tattletech/factcheck-scraper:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 3001
                }
            ]
        }
    ]
}